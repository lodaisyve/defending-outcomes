TITLE:individual-level model 

DATA: 
FILE IS t2t3.dat;

VARIABLE:
   NAMES =  newid clasid clasiz prog
    massd maggd mcomv mrepo mtacd
    mpop mlika
    t2age gender
    t2assd t2aggd t2comv t2repo t2tacd
    t2likp t2popp
    t2vic  t2dep
    t3vic  t3dep
    t2bu_po
    ;


USEVARIABLES = t2age gender t2assd 
t2aggd t2comv t2repo t2tacd
t2likp t2popp t2vic t3vic t2dep t3dep;



CLUSTER = clasid;

WITHIN = t2age gender t2assd 
t2aggd t2comv t2repo t2tacd
t2likp t2popp t2vic t2dep;

BETWEEN = ;

DEFINE: CENTER t2age gender t2assd 
t2aggd t2comv t2repo t2tacd
t2likp t2popp t2vic t2dep (GROUPMEAN);

!center   (grandmean);


ANALYSIS: 
  TYPE = TWOLEVEL random;
  ESTIMATOR = BAYES;
  BITERATIONS = 50000(10000);
  THIN = 20;
  PROCESSORS = 2;
  CHAINS = 3;
  ALGORITHM=GIBBS(RW);

MODEL:
%WITHIN%
t3vic ON t2age gender t2vic t2dep
t2aggd t2comv t2repo t2tacd
t2likp t2popp ;

t3dep ON t2age gender t2vic t2dep
t2aggd t2comv t2repo t2tacd
t2likp t2popp ;

t3vic t3dep;

t2vic WITH t2dep;

s1|t3vic ON t2assd;
s2|t3dep ON t2assd;

%BETWEEN%

s1;
s2;




[t3vic];
t3vic;
[t3dep];
t3dep;



  PLOT:       TYPE = PLOT2;

  OUTPUT:     STDYX SVALUES;
